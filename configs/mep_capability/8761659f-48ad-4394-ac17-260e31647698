  /compkg/v1/basicinfo:
    get:
      tags:
      - "compkg"
      summary: "企业名称精准查询"
      description: "企业名称精准查询"
      consumes:
      - "application/json"
      produces:
      - "application/json"
      parameters:
      - name: "keyword"
        in: "query"
        description: "搜索关键字（公司名、注册号或社会统一信用代码）"
        required: true
        type: "string"
      responses:
        "405":
          description: "Invalid input"
        "200":
          description: "企业名称精准查询成功"  
          schema:
            $ref: "#/definitions/BasicResposeBody"
  /compkg/v1/search:
    get:
      tags:
      - "compkg"
      summary: "企业名称模糊查询"
      description: "企业名称模糊查询"
      consumes:
      - "application/json"
      produces:
      - "application/json"
      parameters:
      - name: "keyword"
        in: "query"
        description: "搜索关键字（公司名、注册号或社会统一信用代码）"
        required: true
        type: "string"
      - name: "cityCode"
        in: "query"
        description: "地级市代码"
        required: false
        type: "string"
      - name: "pageIndex"
        in: "query"
        description: "页码"
        required: false
        type: "integer"
      - name: "pageSize"
        in: "query"
        description: "每页条数"
        required: false
        type: "integer"
      - name: "provinceCode"
        in: "query"
        description: "省份代码"
        required: false
        type: "string"
      responses:
        "405":
          description: "Invalid input"
        "200":
          description: "企业名称模糊查询成功"  
          schema:
            $ref: "#/definitions/SearchResposeBody"
