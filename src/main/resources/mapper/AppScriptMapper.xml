<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.edgegallery.developer.mapper.application.AppScriptMapper">
    <insert id="createAppScript" parameterType="org.edgegallery.developer.model.application.Script">
        insert into tbl_app_script (id, app_id, name, script_file_id, create_time)
        values
        ( #{script.id}, #{applicationId}, #{script.name},
        #{script.scriptFileId}, #{script.createTime})
    </insert>
</mapper>